{
  "name": "grocery-list",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "proxy": "http://0.0.0.0:3001",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CB721/grocery-list.git"
  },
  "author": "Clint Brodar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CB721/grocery-list/issues"
  },
  "homepage": "https://github.com/CB721/grocery-list#readme",
  "dependencies": {
    "@svgr/webpack": "^5.1.0",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "mongoose": "^5.8.9",
    "mysql": "^2.18.1",
    "node-sass": "^4.13.1",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.1.2",
    "react-textfit": "^1.1.0",
    "shards-react": "^1.0.3"
  }
}
